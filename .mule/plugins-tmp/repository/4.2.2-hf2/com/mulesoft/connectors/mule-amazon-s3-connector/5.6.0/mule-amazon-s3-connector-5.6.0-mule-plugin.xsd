<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:mule="http://www.mulesoft.org/schema/mule/core" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.mulesoft.org/schema/mule/s3" attributeFormDefault="unqualified" elementFormDefault="qualified">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"></xs:import>
  <xs:import namespace="http://www.springframework.org/schema/beans" schemaLocation="http://www.springframework.org/schema/beans/spring-beans.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/core" schemaLocation="http://www.mulesoft.org/schema/mule/core/current/mule.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/tls" schemaLocation="http://www.mulesoft.org/schema/mule/tls/current/mule-tls.xsd"></xs:import>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-shared-extension" name="config">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:sequence minOccurs="1" maxOccurs="1">
            <xs:element minOccurs="1" maxOccurs="1" ref="mule:abstractConnectionProvider"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:expiration-policy"></xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>The identifier of this element used to reference it in other components</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstractConnectionProvider" name="basic-connection">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractConnectionProviderType">
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:reconnection"></xs:element>
            <xs:element xmlns:tls="http://www.mulesoft.org/schema/mule/tls" minOccurs="0" maxOccurs="1" ref="tls:context"></xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="optional" name="tlsContext"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="sessionToken"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" default="50000" name="connectionTimeout"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" name="maxConnections"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" default="50000" name="socketTimeout"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="accessKey"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="secretKey"></xs:attribute>
          <xs:attribute type="mule:expressionBoolean" use="optional" default="false" name="tryDefaultAWSCredentialsProviderChain"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" default="US_EAST_1" name="region"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyHost"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" name="proxyPort"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyUsername"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyPassword"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyDomain"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyWorkstation"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="storageUrl"></xs:attribute>
          <xs:attribute type="mule:expressionBoolean" use="optional" default="false" name="forceGlobalBucketAccess"></xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstractConnectionProvider" name="role-connection">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractConnectionProviderType">
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:reconnection"></xs:element>
            <xs:element xmlns:tls="http://www.mulesoft.org/schema/mule/tls" minOccurs="0" maxOccurs="1" ref="tls:context"></xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="optional" name="tlsContext"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="roleARN"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" default="50000" name="connectionTimeout"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" name="maxConnections"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" default="50000" name="socketTimeout"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="accessKey"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="secretKey"></xs:attribute>
          <xs:attribute type="mule:expressionBoolean" use="optional" default="false" name="tryDefaultAWSCredentialsProviderChain"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" default="US_EAST_1" name="region"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyHost"></xs:attribute>
          <xs:attribute type="mule:expressionInt" use="optional" name="proxyPort"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyUsername"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyPassword"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyDomain"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="proxyWorkstation"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="storageUrl"></xs:attribute>
          <xs:attribute type="mule:expressionBoolean" use="optional" default="false" name="forceGlobalBucketAccess"></xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Deleted-object-triggerType" substitutionGroup="mule:abstract-message-source" name="deleted-object-trigger"></xs:element>
  <xs:complexType name="Deleted-object-triggerType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageSourceType">
        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-redelivery-policy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="1" maxOccurs="1" ref="mule:scheduling-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="required" name="bucketName"></xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="notificationQueueName"></xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" name="primaryNodeOnly">
          <xs:annotation>
            <xs:documentation>Whether this source should only be executed on the primary node when runnning in Cluster</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="New-object-triggerType" substitutionGroup="mule:abstract-message-source" name="new-object-trigger"></xs:element>
  <xs:complexType name="New-object-triggerType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageSourceType">
        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-redelivery-policy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="1" maxOccurs="1" ref="mule:scheduling-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="required" name="bucketName"></xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="notificationQueueName"></xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" name="primaryNodeOnly">
          <xs:annotation>
            <xs:documentation>Whether this source should only be executed on the primary node when runnning in Cluster</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="AbortMultipartUploadType" substitutionGroup="mule:abstract-operator" name="abort-multipart-upload">
    <xs:annotation>
      <xs:documentation>Aborts a multipart upload. After a multipart upload is aborted, no additional parts can be uploaded using that upload ID. The storage consumed by any previously uploaded parts will be freed. However, if any part uploads are currently in progress, those part uploads may or may not succeed. As a result, it may be necessary to abort a given multipart upload multiple times in order to completely free all storage consumed by all parts.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AbortMultipartUploadType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="bucketName">
          <xs:annotation>
            <xs:documentation>the name of the bucket containing the multipart upload to abort.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="key">
          <xs:annotation>
            <xs:documentation>the key of the multipart upload to abort.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="uploadId">
          <xs:annotation>
            <xs:documentation>the ID of the upload to abort.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CompleteMultipartUploadType" substitutionGroup="mule:abstract-operator" name="complete-multipart-upload">
    <xs:annotation>
      <xs:documentation>Completes a multipart upload by assembling previously uploaded parts.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CompleteMultipartUploadType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element type="xs:string" minOccurs="0" maxOccurs="1" name="part-e-tags">
            <xs:annotation>
              <xs:documentation>the list of part numbers and ETags that identify the individual parts of the multipart upload to complete.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="bucketName">
          <xs:annotation>
            <xs:documentation>the name of the bucket containing the multipart upload to complete.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="key">
          <xs:annotation>
            <xs:documentation>the key under which the multipart upload to complete is stored.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="uploadId">
          <xs:annotation>
            <xs:documentation>the ID of the multipart upload to complete.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="target">
          <xs:annotation>
            <xs:documentation>The name of a variable on which the operation's output will be placed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CopyObjectType" substitutionGroup="mule:abstract-operator" name="copy-object">
    <xs:annotation>
      <xs:documentation>Copies a source object to a new destination; to copy an object, the caller's account must have read access to the source object and write access to the destination bucket. By default, all object metadata for the source object are copied to the new destination object, unless new object metadata in the specified is provided. The AccessControlList is not copied to the new object, and, unless another ACL specified, PRIVATE is assumed. If no destination bucket is specified, the same that the source bucket is used - local copy. Destination object's Content-Type http header would be set with the specified value and if it is passed null, then the default MIME type 'application/octet-stream' would be set.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CopyObjectType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="destination-user-metadatas">
            <xs:annotation>
              <xs:documentation>the new metadata of the destination object. If specified, overrides that copied from the source object</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="destination-user-metadatum">
                  <xs:complexType>
                    <xs:attribute type="mule:expressionString" use="required" name="key"></xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="value"></xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="sourceBucketName">
          <xs:annotation>
            <xs:documentation>the source object's bucket</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="sourceKey">
          <xs:annotation>
            <xs:documentation>the source object's key</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="sourceVersionId">
          <xs:annotation>
            <xs:documentation>the specific version of the source object to copy, if versioning is enabled.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="destinationBucketName">
          <xs:annotation>
            <xs:documentation>the destination object's bucket. If none provided, object is copied within the same bucket.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="destinationKey">
          <xs:annotation>
            <xs:documentation>the destination object's key</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/s3" type="org.mule.extension.s3.api.model.CannedAccessControlListEnumType" use="optional" default="PRIVATE" name="destinationAcl">
          <xs:annotation>
            <xs:documentation>the acl of the destination object.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/s3" type="org.mule.extension.s3.api.model.StorageClassEnumType" use="optional" default="STANDARD" name="destinationStorageClass">
          <xs:annotation>
            <xs:documentation>one of StorageClass enumerated values, defaults to StorageClass#STANDARD</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionMap" use="optional" name="destinationUserMetadata">
          <xs:annotation>
            <xs:documentation>the new metadata of the destination object. If specified, overrides that copied from the source object</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionDateTime" use="optional" name="modifiedSince">
          <xs:annotation>
            <xs:documentation>The modified constraint that restricts this request to executing only if the object has been modified after the specified date.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionDateTime" use="optional" name="unmodifiedSince">
          <xs:annotation>
            <xs:documentation>The unmodified constraint that restricts this request to executing only if the object has not been modified after this date.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="encryption">
          <xs:annotation>
            <xs:documentation>Encryption method for server-side encryption. Supported value AES256.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="contentType">
          <xs:annotation>
            <xs:documentation>the destination object's Content-Type HTTP header</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="target">
          <xs:annotation>
            <xs:documentation>The name of a variable on which the operation's output will be placed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CreateBucketType" substitutionGroup="mule:abstract-operator" name="create-bucket">
    <xs:annotation>
      <xs:documentation>Creates a new bucket; connector must not be configured as anonymous for this operation to succeed. Bucket names must be unique across all of Amazon S3, that is, among all their users. Bucket ownership is similar to the ownership of Internet domain names. Within Amazon S3, only a single user owns each bucket. Once a uniquely named bucket is created in Amazon S3, organize and name the objects within the bucket in any way. Ownership of the bucket is retained as long as the owner has an Amazon S3 account. To conform with DNS requirements, buckets names must: not contain underscores, be between 3 and 63 characters long, not end with a dash, not contain adjacent periods, not contain dashes next to periods and not contain uppercase characters. Do not make bucket create or delete calls in the high availability code path of an application. Create or delete buckets in a separate initialization or setup.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateBucketType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="bucketName">
          <xs:annotation>
            <xs:documentation>The bucket to create. It must not exist yet.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="US_STANDARD" name="region">
          <xs:annotation>
            <xs:documentation>the region where to create the new bucket</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/s3" type="org.mule.extension.s3.api.model.CannedAccessControlListEnumType" use="optional" default="PRIVATE" name="acl">
          <xs:annotation>
            <xs:documentation>the access control list of the new bucket</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="target">
          <xs:annotation>
            <xs:documentation>The name of a variable on which the operation's output will be placed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/s3" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CreateObjectType" substitutionGroup="mule:abstract-operator" name="create-object">
    <xs:annotation>
      <xs:documentation>Uploads an object to S3. Supported contents are InputStreams, Strings, byte arrays and Files.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateObjectType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element type="xs:string" minOccurs="0" maxOccurs="1" name="object-content">
            <xs:annotation>
              <xs:documentation>the content to be uploaded to S3, capable of creating a PutObjectRequest.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="user-metadatas">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="user-metadatum">
                  <xs:complexType>
                    <xs:attribute type="mule:expressionString" use="required" name="key"></xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="value"></xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="bucketName">
          <xs:annotation>
            <xs:documentation>the object's bucket</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="key">
          <xs:annotation>
            <xs:documentation>the object's key</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/s3" type="org.mule.extension.s3.api.model.CannedAccessControlListEnumType" use="optional" default="PRIVATE" name="acl">
          <xs:annotation>
            <xs:documentation>the access control list of the new object</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="kmsMasterKey">
          <xs:annotation>
            <xs:documentation>Encrypt objects uploaded to S3 buckets with AWS KMS master key</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
